{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "framework": null,
    "devCommand": "vite",
    "buildCommand": "vite build",
    "installCommand": "pnpm install",
    "outputDirectory": "build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}
